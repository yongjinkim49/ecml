{
    "hp_cfg": {
        "model": "LeNet-5",
        "hyperparams": {
            "window_size": {
                "value_type": "discrete",
                "range": [2, 10],
                "type": "int"
            },
            "p1_size": {
                "value_type": "discrete",
                "range": [2, 3],
                "type": "int"
            },
            "c2_depth": {
                "value_type": "discrete",
                "range": [1, 350],
                "type": "int"
            },
            "learning_rate": {
                "value_type": "continuous",
                "range": [0.0001, 1.0],
                "type": "float"
            },
            "f1_width": {
                "value_type": "discrete",
                "range": [1, 1024],
                "type": "int"
            },
            "c1_depth": {
                "value_type": "discrete",
                "range": [1, 350],
                "type": "int"
            },
            "p2_size": {
                "value_type": "discrete",
                "range": [2, 3],
                "type": "int"
            },
            "keep_prop_rate": {
                "value_type": "continuous",
                "range": [0.1, 1.0],
                "type": "float"
            },
            "reg_param": {
                "value_type": "continuous",
                "range": [0.0, 1.0],
                "type": "float"
            }
        },
        "config": {
            "max_epoch": 15,
            "surrogate": "data1"
        },
        "param_order": ["c1_depth", "p1_size", "c2_depth", "p2_size", "f1_width", "window_size", "learning_rate", "reg_param", "keep_prop_rate"],
        "dataset": "MNIST"
    },

    "exp_crt": "TIME",
    "exp_time": 86400,
    "exp_goal": 0.999,
    "num_trials": 1,    
    "mode": "DIV",
    "spec": "SEQ",

    "surrogate": "data1",
    "worker_url": "none"    
}